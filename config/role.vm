<tr>
<td align="left" style="border: none;">
	<table class="outsideStdTable" style="margin-top: 10px; margin-bottom: 10px;">
	<tr><td>
	<table class="stdTable">
		<tr>
			<td style="font-size: 12pt; font-weight: bold;" width="90">Physical</td>
			<td style="font-size: 10pt; font-style: italic;" width="60">Max: $!role.Generation.TraitsMax</td>
			<td width="50" align="center" style="font-size: 12pt;">$role.Physical</td>
		</tr>
	</table>
	</td></tr>
	</table>
</td>
<td align="center" style="border: none;">
	<table class="outsideStdTable" style="margin-top: 10px; margin-bottom: 10px;">
	<tr><td>
	<table class="stdTable">
		<tr>
			<td style="font-size: 12pt; font-weight: bold;" width="90">Social</td>
			<td style="font-size: 10pt; font-style: italic;" width="60">Max: $!role.Generation.TraitsMax</td>
			<td width="50" align="center" style="font-size: 12pt;">$role.Social</td>
		</tr>
	</table>
	</td></tr>
	</table>
</td>
<td align="right" style="border: none;">
	<table class="outsideStdTable" style="margin-top: 10px; margin-bottom: 10px;">
	<tr><td>
	<table class="stdTable">
		<tr>
			<td style="font-size: 12pt; font-weight: bold;" width="90">Mental</td>
			<td style="font-size: 10pt; font-style: italic;" width="60">Max: $!role.Generation.TraitsMax</td>
			<td width="50" align="center" style="font-size: 12pt;">$role.Mental</td>
		</tr>
	</table>
	</td></tr>
	</table>
</td>
</tr>
<tr>
#set($toManyMnFs = false)
#if(($role.Merits.size() + $role.Flaws.size()) >= 9)
    #set($toManyMnFs = true)
#end
<td style="border: none;" colspan="3">
	<table class="plain" cellpadding="0" cellspacing="0" border="0" width="100%">
	<tr>
			#if($toManyMnFs)
		    <td style="border: none;" valign="top" align="left" width="25%">
		    #else
		    <td style="border: none;" valign="top" align="left" width="33%">
		    #end
                <table class="outsideStdTable" width="90%">
                <tr><td>
                <table border="0" cellspacing="0" cellpadding="1" class="stdTable" width="100%">
                  <tr>
                    <td colspan="2"><b>Disciplines</b></td>
                  </tr>
                  #foreach($di in $role.Disciplines)
                  <tr>
                    <td>$di</td>
                    <td width="10%">
                        #if($di.Dots > 0)
                        $di.Dots
                        #else
                        &nbsp;
                        #end
                    </td>
                  </tr>
                  #end
                  <tr>
                    <td>&nbsp;</td>
                    <td width="10%">&nbsp;</td>
                  </tr>
                </table>
                #if($role.hasThaumaturgy())
                <table border="0" cellspacing="0" cellpadding="2" class="stdTable" width="100%" style="margin-top: 1px;">
                <tr>
                    <td colspan="2">
                        <b>Thaumaturgy</b>
                        #if($role.ThaumaType.length() > 0)
                        <br/><span class="smallComment">($role.ThaumaType)</span>
                        #end
                    </td>
                  </tr>
                  #foreach($path in $role.ThaumaturgicalPaths)
                  <tr>
                    <td>$path</td>
                    <td width="10%">$!path.Dots</td>
                  </tr>
                  #end
                  <tr>
                    <td>&nbsp;</td>
                    <td width="10%">&nbsp;</td>
                  </tr>
                </table>
                #end
                #if($role.hasNecromancy())
                <table border="0" cellspacing="0" cellpadding="2" class="stdTable" width="100%" style="margin-top: 1px;">
                <tr>
                    <td colspan="2">
                        <b>Necromancy</b>
                        #if($role.NecromancyType.length() > 0)
                        <br/><span class="smallComment">($role.NecromancyType)</span>
                        #end
                    </td>
                  </tr>
                  #foreach($path in $role.NecromancyPaths)
                  <tr>
                    <td>$path</td>
                    <td width="10%">$!path.Dots</td>
                  </tr>
                  #end
                  <tr>
                    <td>&nbsp;</td>
                    <td width="10%">&nbsp;</td>
                  </tr>
                </table>
                #end
                </td></tr>
                </table>
		    </td>
		    #if($toManyMnFs)
		    <td style="border: none;" valign="top" align="center" width="25%">
		    #else
		    <td style="border: none;" valign="top" align="center" width="33%">
		    #end
                <table class="outsideStdTable" width="90%">
                <tr><td>
                <table border="0" cellspacing="0" cellpadding="2" class="stdTable" width="100%">
                <tr>
                    <td>
                        #macro(box)
                        <div class="healthBox"> </div>
                        #end
                        <b>Health Level</b>
                        <table cellpadding="2" cellspacing="0" border="0" class="plain" style="border: none;">
                            #if($role.ExtraHealthLevels > 0)
                                #foreach($i in [1..$role.ExtraHealthLevels])
                                <tr>
                                    <td style="border: none;">#box()</td>
                                    <td colspan="2" style="border: none;">Healthy</td>
                                </tr>
                                #end
                            #end
                            <tr>
                                <td style="border: none;">#box()</td>
                                <td colspan="2" style="border: none;">Healthy</td>
                            </tr>
                            <tr>
                                <td style="border: none;">#box()</td>
                                <td colspan="2" style="border: none;">Healthy<br/>
                            </tr>
                            <tr>
                                <td style="border: none;">#box()</td>
                                <td style="border: none;">
                                    Bruised
                                </td>
                                <td style="border: none;">
                                    #if($role.SufferesOfInjury) -1 #else &nbsp; #end
                                </td>
                            </tr>
                            <tr>
                                <td style="border: none;">#box()</td>
                                <td colspan="2" style="border: none;">Bruised</td>
                            </tr>
                            <tr>
                                <td style="border: none;">#box()</td>
                                <td colspan="2" style="border: none;">Bruised</td>
                            </tr>
                            <tr>
                                <td style="border: none;">#box()</td>
                                <td style="border: none;">
                                    Wounded
                                </td>
                                <td style="border: none;">
                                    #if($role.SufferesOfInjury) -2 #else &nbsp; #end
                                </td>
                            </tr>
                            <tr>
                                <td style="border: none;">#box()</td>
                                <td colspan="2" style="border: none;">Wounded</td>
                            </tr>
                            <tr>
                                <td style="border: none;">#box()</td>
                                <td colspan="2" style="border: none;">Incapacitated</td>
                            </tr>
                            <tr>
                                <td style="border: none;">#box()</td>
                                <td colspan="2" style="border: none;">Torpor or Final Death</td>
                            </tr>
                        </table>
                    </td>
                </tr>
                </table>#*<!--
                <table border="0" cellspacing="0" cellpadding="2" class="stdTable" width="100%" style="margin-top: 1px;">
                  <tr>
                    <td colspan="2"><b>Other Traits</b></td>
                  </tr>
                  #set($traits = $role.OtherTraits)
                  #foreach($trait in $traits)
                  <tr>
                    <td>$trait</td>
                    <td width="10%">$!trait.Dots</td>
                  </tr>
                  #end
                  <tr>
                    <td>&nbsp;</td>
                    <td width="10%">&nbsp;</td>
                  </tr>
                </table>-->*#
                </td></tr>
                </table>
		    </td>
		    #if($toManyMnFs)
		    <td style="border: none;" valign="top" align="right" width="25%">
		        <table class="outsideStdTable" width="90%">
                <tr><td>
                <table border="0" cellspacing="0" cellpadding="2" class="stdTable" width="100%">
                  <tr>
                    <td><b>Merits</b></td>
                  </tr>
                  #set($merits = $role.Merits)
                  #foreach($merit in $merits)
                  <tr>
                    <td>
                        $!merit.Name
                        #if($merit.Note.length() > 0)
                            <span class="noteId">$notes.addNote( $merit.Note )</span>
                        #end
                    </td>
                  </tr>
                  #end
                  <tr>
                    <td>&nbsp;</td>
                  </tr>
                </table>
                </td></tr>
                </table>
		    </td>
		    <td style="border: none;" valign="top" align="right" width="25%">
		        <table class="outsideStdTable" width="90%">
                <tr><td>
                <table border="0" cellspacing="0" cellpadding="2" width="100%" class="stdTable">
                  <tr>
                    <td><b>Flaws</b></td>
                  </tr>
                  #set($merits = $role.Flaws)
                  #foreach($merit in $merits)
                  <tr>
                    <td>
                        $!merit.Name
                        #if($merit.Note.length() > 0)
                            <span class="noteId">$notes.addNote( $merit.Note )</span>
                        #end
                    </td>
                  </tr>
                  #end
                  <tr>
                    <td>&nbsp;</td>
                  </tr>
                </table>
                </td></tr>
                </table>
		    </td>
		    #else
		    <td style="border: none;" valign="top" align="right" width="33%">
