<body bgcolor="#FFFFFF" text="#000000">
#foreach( $role in $roles )
<div class="paper" valign="top">
#set($clanSymbol = $ClanSymbolFinder.get($role.Clan))
<!-- <div align="center"> -->
<table class="plain" border="0" cellspacing="0" cellpadding="0" width="100%" style="border: none; margin-top: 0px;">
<tr>
    <td align="left" class="clanSymbol"  style="border: none;" valign="top">
        &nbsp;$!clanSymbol
    </td>
    <td align="center"  style="border: none;">
        <p class="headingTitle" align="center">$heading</p>
        <p align="center" style="padding-bottom: 0.5cm;">
        	<span class="roleName">
        		<b><i>$role.Name</i></b>
        	</span>
        </p>
    </td>
    <td align="right" class="clanSymbol"  style="border: none;" valign="top">
        $!clanSymbol&nbsp;
    </td>
</tr>
</table>
  <!--<p class="headingTitle" align="center">$heading</p>
    <p align="center">
    	<span class="roleName">
    		<b><i>$role.Name</i></b>
    	</span>
    </p>-->
<!-- </div> -->
<table class="plain" border="0" cellspacing="0" cellpadding="2" width="100%">
<tr vAlign="top">
<td style="border: none;" align="left">
	<table class="outsideStdTable">
	<tr><td>
	<table border="0" cellspacing="0" cellpadding="2" width="198" class="stdTable">
	  <tr>
		<td><b>Player:</b> $role.PlayerName</td>
	  </tr>
	  #if( !$role.isGhoul() )
	  <tr>
		<td><b>Generation:</b> $role.Generation</td>
	  </tr>
	  #else
	  <tr>
		<td><b>Ghoul</b></td>
	  </tr>
	  #end
	  #if( !$role.isGhoul() )
	  <tr>
		<td><b>Embraced:</b> $role.EmbracedString</td>
	  </tr>
	  #end
	  <tr>
		<td><b>Age:</b> $role.Age</td>
	  </tr>
	  <tr>
		<td>
			#if( !$role.isGhoul() )
			<b>Sire:</b>
			#else
			<b>Master:</b>
			#end
			$!role.Sire
		</td>
	  </tr>
	  <tr>
		<td><b>Clan:</b> $role.Clan</td>
	  </tr>
	  <tr>
		<td><b>Demeanor:</b> $role.Demeanor</td>
	  </tr>
	  <tr>
		<td><b>Nature:</b> $role.Nature</td>
	  </tr>
	</table>
	</td></tr>
	</table>
</td>
<td style="border: none;" align="center">
	<table class="outsideStdTable">
	<tr><td>
	<table border="0" cellspacing="0" cellpadding="2" class="stdTable" width="198">
	    <tr>
			<td colspan="2" width="100%">
			        <b>Domain:</b>&nbsp;
			    $role.Domain.Name
		    </td>
		</tr>
		<tr>
			<td width="90%"><b>Courage:</b></td>
			<td width="10%" align="center" nowrap>$role.Courage</td>
		</tr>
		<tr>
			<td width="90%"><b>$role.ConcienseORconviction:</b></td>
			<td width="10%" nowrap align="center">$role.Concience</td>
		</tr>
		<tr>
			<td width="90%"><b>$role.SelfControlORinstinct:</b></td>
			<td width="10%" nowrap align="center">$role.SelfControl</td>
		</tr>
		<tr>
			<td width="90%"><b>Willpower:</b></td>
			<td width="10%" nowrap align="center"> $!role.Willpower/$!role.Generation.WillpowerMax </td>
		</tr>
		<tr>
			<td width="90%"><b>Bloodpool:</b></td>
			<td width="10%" nowrap align="center">
			    $!role.Generation.BloodPool
			    #if($role.Generation.HumanBlood > 0)/$role.Generation.HumanBlood #end
			    /$!role.Generation.SpendBlood
			</td>
		</tr>
		<tr>
			<td width="90%"><b>$role.Path:</b></td>
			<td width="10%" nowrap align="center">$role.PathDots</td>
		</tr>
	</table>
	</td></tr>
	</table>
</td>
<td style="border: none;" align="right">
	<table class="outsideStdTable">
	<tr><td>
	<table border="0" cellspacing="0" cellpadding="2" width="198" class="stdTable" width="198">
	#set( $derangements = $role.Derangements )
	#set( $beastTraits = $role.BeastTraits )
	#set( $emptyRows = 7 - $derangements.size() - $beastTraits.size() )
	#if( $derangements.size() > 0 && $beastTraits.size() > 0 )
	    #set ($emptyRows =  $emptyRows - 2)
	#end
	#if($derangements.size() > 0)
	  <tr>
		<td><b>Derangements</b></td>
	  </tr>
	  #foreach($deran in $derangements )
	  <tr>
		<td>$deran</td>
	  </tr>
	  #end
	  #if($beastTraits.size() > 0)
	  <tr>
		<td>&nbsp;</td>
	  </tr>
	  #end
	#end
	#if($beastTraits.size() > 0)
	  <tr>
		<td><b>Beast Traits</b></td>
	  </tr>
	  #foreach($deran in $beastTraits )
	  <tr>
		<td>$deran</td>
	  </tr>
	  #end
	#end
	  #foreach($i in [1..$emptyRows])
	  <tr>
		<td>&nbsp;</td>
	  </tr>
	  #end
	</table>
	</td></tr>
	</table>
</td>
</tr>
<tr>
<td style="border: none;" colspan="3">
	<table class="outsideStdTable" width="100%" style="margin-top: 10px;">
	<tr><td>
	<table width="100%" cellspacing="0" border="0" cellpadding="0">
	<tr>
	#set($abilities = $role.PhysicalAbilities)
	#set ($maxRows = $abilities.size())
	#if($maxRows < $role.SocialAbilities.size())
		#set($maxRows = $role.SocialAbilities.size())
	#end
	#if($maxRows < $role.MentalAbilities.size())
		#set($maxRows = $role.MentalAbilities.size())
	#end
	#set($extraRows = $maxRows - $abilities.size())
	<td width="33%" style="border: none; padding-right: 1px;" valign="top">
		<table border="0" cellspacing="0" cellpadding="2" width="100%" class="stdTable">
		  <tr>
			<td colspan="2" class="abilityCell">
				<b>
					Abilities
					#if( !$role.isGhoul() )
						(Max: $!role.Generation.AbilitiesMax)
					#end
				</b>
			</td>
		  </tr>                     <!-- PHYSICAL ABILITIES-->
		  #foreach($ability in $abilities)
		  <tr>
			<td class="abilityCell">
				$ability
				#if($ability.Specialisation.length() > 11)
                	( <span class="abilityNote">$notes.addNote( $ability.Specialisation )</span>  )
				#elseif($ability.Specialisation.length() > 0)
				( $ability.Specialisation )
				#end
			</td>
			<td width="10%" class="abilityCell">#if( $ability.Dots > 0 ) $ability.Dots #end</td>
		  </tr>
		  #end
		  #if($extraRows > 0)
		  	#foreach($i in [1..$extraRows])
		  <tr>
			<td class="abilityCell">&nbsp;</td>
			<td width="10%" class="abilityCell">&nbsp;</td>
		  </tr>
		  	#end
		  #end
		</table>
	</td>
	<td width="33%" style="border: none; padding-right: 1px;" valign="top">
		<table border="0" cellspacing="0" cellpadding="2" width="100%" class="stdTable">
		  <tr>
			<td colspan="2" class="abilityCell">
				<b>
					Abilities
					#if( !$role.isGhoul() )
						(Max: $!role.Generation.AbilitiesMax)
					#end
				</b>
			</td>
		  </tr>                 <!-- SOCIAL ABILITIES-->
		  #set($abilities = $role.SocialAbilities)
		  #set($extraRows = $maxRows - $abilities.size())
		  #foreach($ability in $abilities)
		  <tr>
			<td class="abilityCell">
				$ability
				#if($ability.Specialisation.length() > 11)
                	( <span class="abilityNote">$notes.addNote( $ability.Specialisation )</span> )
				#elseif($ability.Specialisation.length() > 0)
				( $ability.Specialisation )
				#end
			</td>
			<td width="10%" class="abilityCell">#if( $ability.Dots > 0 ) $ability.Dots #end</td>
		  </tr>
		  #end
		  #if($extraRows > 0)
		  	#foreach($i in [1..$extraRows])
		  <tr>
			<td class="abilityCell">&nbsp;</td>
			<td width="10%" class="abilityCell">&nbsp;</td>
		  </tr>
		  	#end
		  #end
		</table>
	</td>
	<td width="33%" style="border: none;" valign="top">
		<table border="0" cellspacing="0" cellpadding="2" width="100%" class="stdTable">
		  <tr>
			<td colspan="2" class="abilityCell">
				<b>
					Abilities
					#if( !$role.isGhoul() )
						(Max: $!role.Generation.AbilitiesMax)
					#end
				</b>
			</td>
		  </tr>                 <!-- MENTAL ABILITIES-->
		  #set($abilities = $role.MentalAbilities)
		  #set($extraRows = $maxRows - $abilities.size())
		  #foreach($ability in $abilities)
		  <tr>
			<td class="abilityCell">
				$ability
				#if($ability.Specialisation.length() > 11)
                	( <span class="abilityNote">$notes.addNote( $ability.Specialisation )</span> )
				#elseif($ability.Specialisation.length() > 0)
				( $ability.Specialisation )
				#end
			</td>
			<td width="10%" class="abilityCell">#if( $ability.Dots > 0 ) $ability.Dots #end</td>
		  </tr>
		  #end
		  #if($extraRows > 0)
		  	#foreach($i in [1..$extraRows])
		  <tr>
			<td class="abilityCell">&nbsp;</td>
			<td width="10%" class="abilityCell">&nbsp;</td>
		  </tr>
		  	#end
		  #end
		</table>
	</td>
	</tr>
	</table>
	#if($role.OtherTraits.size() > 0)
	<table border="0" cellspacing="0" cellpadding="2" width="100%" class="stdTable" style="margin-top: 1px;">
    <tr>
        <td colspan="12" class="otherTraitCell"><b>Other Traits</b></td>
    </tr>
    <tr>
        #set($left = 5)
		#foreach($trait in $role.OtherTraits)
		    <td width="10%" class="otherTraitCell" nowrap="true">$trait</td>
			<td width="1%" class="otherTraitCell" nowrap="true">$trait.Dots</td>
			#set($left = $left - 1)
			#if($velocityCount % 6 == 0 && $velocityCount < $role.OtherTraits.size() )
			#set($left = 5)
	</tr>
	<tr>
            #end
		#end
		#if($left >= 0)
		#foreach($i in [0..$left])
		    <td width="10%" class="otherTraitCell">&nbsp;</td>
			<td width="1%" class="otherTraitCell">&nbsp;</td>
		#end
		#end
    </tr>
    </table>
    #end
	</td></tr>
	</table>
</td>
</tr>
#*
